package com.withearth.tumbler.controller;

import org.springframework.stereotype.Controller;
import org.springframework.ui.Model;
import org.springframework.web.bind.annotation.RequestParam;

import com.withearth.tumbler.service.TumListService;

// 텀블리 이용리스트 컨트롤러
@Controller
public class TumListController {

	private TumListService tumlistservice;
	
	//@RequestMapping("/qr/tumlist")
	public String tumList(
			@RequestParam(value = "p", defaultValue="1") int page,
			SearchParam param,
			Model model
			) {
		
		model.addAttribute("listView", tumlistService.getListView(param));
		
		return"qr/tumlist";
	}
	
	
}
