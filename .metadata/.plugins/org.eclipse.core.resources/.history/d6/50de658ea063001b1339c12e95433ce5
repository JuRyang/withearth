package com.withearth.tumbler.controller;

import java.io.File;

import org.springframework.web.bind.annotation.RequestMapping;

@RequestMapping(value="/makeqr")
public class makeqr {
	
	String url = "http://roqkffhwk.tistory.com";
	int width = 50;
	int height = 50;
	
	String file_path = "C:"+File.separator+"qr"+File.separator;
	String file_name = "myblog.png";
	QRUtil.makeQR(url, width, height, file_path, file_name);

}
